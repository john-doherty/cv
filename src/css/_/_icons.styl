@font-face {
	font-family: 'icons';
	src: url('fonts/icons.eot?4rr5ai');
	src:  url('fonts/icons.eot?4rr5ai#iefix') format('embedded-opentype'),
	url('fonts/icons.ttf?4rr5ai') format('truetype'),
	url('fonts/icons.woff?4rr5ai') format('woff'),
	url('fonts/icons.svg?4rr5ai#icons') format('svg');
	font-weight: normal;
	font-style: normal;
}

.icon {
	/* use !important to prevent issues with browser extensions that change fonts */
	font-family: 'icons' !important;
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon--open_in_new:before {
	content: "\e89e";
}